{
    "name": "account-manager-app",
    "version": "0.0.0",
    "license": "ISC",
    "workspaces": [
      "api/*",
      "ui/*",
      "docs/*"
    ],
    "scripts": {
      "clean": "yarn run clean-turbo && yarn run clean-node-modules",
      "clean-node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
      "clean-turbo": "turbo run clean --cache-dir .turbo && rm -rf .turbo",
      "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
    },
    "dependencies": {
      "prettier": "latest",
      "turbo": "^1.9.8"
    },
    "engines": {
      "node": ">=16",
      "npm": ">=9.0.0"
    }
  }
  